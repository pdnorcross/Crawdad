<h1>All Customers</h1>
<%= link_to 'New customer', new_customers_path %>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>

  <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.fname %></td>
        <td><%= customer.lname %></td>
      </tr>
  <% end %>
</table>